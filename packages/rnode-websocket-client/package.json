{
  "name": "@rnode-server/websocket-client",
  "version": "0.5.0",
  "description": "WebSocket client for RNode Server with room support and auto-reconnection",
  "type": "module",
  "main": "./dist/rnode-websocket-client.umd.js",
  "module": "./dist/rnode-websocket-client.es.js",
  "unpkg": "./dist/rnode-websocket-client.iife.js",
  "jsdelivr": "./dist/rnode-websocket-client.iife.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rnode-websocket-client.es.js",
      "require": "./dist/rnode-websocket-client.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "build:tsc": "tsc",
    "dev": "vite",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "websocket",
    "rnode",
    "realtime",
    "rooms",
    "auto-reconnect",
    "typescript",
    "client"
  ],
  "author": "RNode Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rnode/rnode-server.git"
  },
  "bugs": {
    "url": "https://github.com/rnode/rnode-server/issues"
  },
  "homepage": "https://github.com/rnode/rnode-server#readme",
  "devDependencies": {
    "@types/node": "^20.19.11",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
